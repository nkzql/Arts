<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finalexamart">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.MyTheme">
        <activity
            android:name="com.example.finalexamart.activity.MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.XINanActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"/>
        <activity
            android:name=".activity.ContentActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"/>
        <activity
            android:name=".activity.ImagesActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"/>
        <activity
            android:name=".activity.CardActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"/>
    </application>
</manifest>